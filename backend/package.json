{
  "name": "mgdc-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "author": "Firdaws Aboulaye",
  "license": "ISC",
  "scripts": {
    "download:thumbnails": "node thumbnails/async.js",
    "resize:thumbnails": "node thumbnails/async-resizing.js",
    "package:db": "sam package --config-env db",
    "deploy:db": "sam deploy --config-env db --no-fail-on-empty-changeset",
    "push:db": "npm run package:db && npm run deploy:db",
    "package:mgdc": "sam package --config-env mgdc",
    "deploy:mgdc": "sam deploy --config-env mgdc --no-fail-on-empty-changeset",
    "push:mgdc": "npm run package:mgdc && npm run deploy:mgdc",
    "package:chat": "sam package --config-env chat",
    "deploy:chat": "sam deploy --config-env chat --no-fail-on-empty-changeset",
    "push:chat": "npm run package:chat && npm run deploy:chat",
    "package:breed": "sam package --config-env breed",
    "deploy:breed": "sam deploy --config-env breed --no-fail-on-empty-changeset",
    "push:breed": "npm run package:breed && npm run deploy:breed",
    "push:all": "npm run push:db && npm run push:mgdc && npm run push:chat && npm run push:breed"
  },
  "devDependencies": {
    "async": "^3.2.3",
    "request": "^2.88.2",
    "request-progress": "^3.0.0",
    "sharp": "^0.30.3"
  }
}
